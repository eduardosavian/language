for (int i = 1; i <= 100; ++i) {
        if (i % 3 == 0 && i % 5 == 0) {
            // If i is divisible by both 3 and 5, print "FizzBuzz"
            std::cout << "FizzBuzz";
        } else if (i % 3 == 0) {
            // If i is divisible by 3, print "Fizz"
            std::cout << "Fizz";
        } else if (i % 5 == 0) {
            // If i is divisible by 5, print "Buzz"
            std::cout << "Buzz";
        } else {
            // Otherwise, print the number i
            std::cout << i;
        }

        // Print a newline after each number
        std::cout << std::endl;
    }
